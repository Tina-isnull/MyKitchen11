<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/lo_ri_select"
    tools:context=".activity.LogoRegistActivity">

    <Button
        android:id="@+id/btn_lore_reId"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_above="@+id/btn_lore_loId"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/btn_background_selector"
        android:gravity="center"
        android:text="急速注册"
        android:textColor="@color/lr_font_color_selector" />

    <Button
        android:id="@+id/btn_lore_loId"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="40dp"
        android:background="@drawable/btn_background_selector"
        android:gravity="center"
        android:text="登录"
        android:textColor="@color/lr_font_color_selector" />
</RelativeLayout>
